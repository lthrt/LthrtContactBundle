{% extends 'LthrtContactBundle:County:county.html.twig' %}

{% block body -%}
    <h1>County</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Active</th>
                <td>{{ county.active }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ county.created|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ county.updated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ county.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ county.name }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('county_list') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('county_edit', { 'county' : county.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock body %}
