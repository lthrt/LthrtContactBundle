{% extends 'LthrtContactBundle:ContactType:contacttype.html.twig' %}

{% block body -%}
    <h1>ContactType</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Active</th>
                <td>{{ contacttype.active }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ contacttype.created|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ contacttype.updated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ contacttype.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ contacttype.name }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('contacttype') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contacttype_edit', { 'contacttype' : contacttype.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock body %}
