$("a.modal-link")
    .on("click",
        function(e) {
            e.preventDefault();
           var url= $(this)
                .attr("href");
                console.log(url);
            $.ajax({
                url: url,
                data: {},
                dataType: 'html',
                type: 'GET',
                cache: false,
                beforeSend: function(xhr) {
                    console.log(url);
                },
                success: function(responseText, textStatus, XMLHttpRequest) {
                    console.log($(responseText));
                    console.log(responseText);
                    console.log($(responseText).find('#modal'));
                    $('#modal').replaceWith(responseText);
                },
                complete: function() {
                    $('#modal')
                        .modal('show');
                }
            });
        }
    );