{% extends 'LthrtContactBundle:City:city.html.twig' %}

{% block body -%}
    <h1>City</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Active</th>
                <td>{{ city.active }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ city.created|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ city.updated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ city.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ city.name }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('city') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('city_edit', { 'city' : city.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock body %}
